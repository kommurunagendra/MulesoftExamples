<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cgi.charm.sc.tlc.persistence.model">

	<class name="com.cgi.charm.sc.tlc.persistence.model.TlcDeviceSubscriptions"
		table="tlc_device_subscription_info">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="tlcId" column="tlc_id" />
		<property name="subscriptionId" column="subscription_id" />
	</class>
</hibernate-mapping>