<?xml version="1.0" encoding="UTF-8"?>
<tlc_error_xml>
		<error_message>
		<xsl:choose>
				<xsl:when test="#[flowVars.errorMsg]">
					<xsl:value-of select="#[flowVars.errorMsg]"/>
				</xsl:when>
				<xsl:otherwise><xsl:text>unknown error occured</xsl:text></xsl:otherwise>
				</xsl:choose>
	</error_message>
	<xml_content>
		#[flowVars.varRquestXml]
	</xml_content>
</tlc_error_xml>