<?xml version="1.0" encoding="UTF-8"?>
<tlc_inventory_subscription version="1.0.0" xmlns="http://tlc.cdm.charm.cgi.com">
	<meta>
		<message_id>
			<uuid>#[function:uuid]</uuid>
		</message_id>
		<message_create_date>#[org.apache.commons.lang.StringUtils.substringBefore(server.dateTime.toString(), ".")+'Z']</message_create_date>
	</meta>
	<subscription_config>
		<subscription_id>
			<uuid>#[payload]</uuid>
		</subscription_id>
		<subscription_attr>
			<subscription_name>subscription_name</subscription_name>
			<subscription_type>inventory</subscription_type>
		</subscription_attr>
		<all_tlcs>true</all_tlcs>
	</subscription_config>
</tlc_inventory_subscription>
