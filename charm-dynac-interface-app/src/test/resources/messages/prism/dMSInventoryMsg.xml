			<station-line-position>0</station-line-position>
			<fep-lines>2</fep-lines>
			<position-bps-code>code</position-bps-code>
			<data-owner>dynac</data-owner>
		</device-inventory-header>
		<dms-sign-type>variable message sign VMS</dms-sign-type>
		<prism-type-id>id2</prism-type-id>
	</dms-inventory-item>
