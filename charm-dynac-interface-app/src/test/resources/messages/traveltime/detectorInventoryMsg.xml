			xsi:type="ns2:DeviceInventoryHeaderCharm">
			<organization-information>
				<organization-id>RWS</organization-id>
			</organization-information>
			<device-id>traveltimeId1</device-id>
			<device-name>traveltimeName1</device-name>
			<device-description>traveltimeDescription1</device-description>
			<device-type>traveltime</device-type>
