<ns2:detectorInventoryMsg xmlns:ns2="http://www.tmdd.org/3/messages"
	xmlns:ns3="http://www.ntcip.org/c2c-message-administration">
	<detector-inventory-item xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
