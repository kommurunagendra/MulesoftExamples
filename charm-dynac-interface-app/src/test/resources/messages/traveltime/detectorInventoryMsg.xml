		</geolocation-list>
		<carriageway>connectingCarriageway</carriageway>
		<measurement-equipment-ref>equipment_reference</measurement-equipment-ref>
		<measurement-equipment-type>bluetooth</measurement-equipment-type>
		<traveltime-related-detector-list>
			<list-item>detectorId1</list-item>
		</traveltime-related-detector-list>
